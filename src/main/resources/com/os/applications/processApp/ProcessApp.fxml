<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="topMainPane" fx:id="topMainPane" prefHeight="494.0" prefWidth="1024.0" stylesheets="@ProcessApp.css"
            xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.os.applications.processApp.ProcessAppController">
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <BorderPane fx:id="titleBar" layoutX="40.0" layoutY="40.0" stylesheets="@ProcessApp.css"
                onDragDone="#dragBarDone" onMouseDragged="#dragBar"
                onMousePressed="#pressBar" onMouseReleased="#releaseBar"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <left>
            <HBox fx:id="titleBarL" alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="134.0"
                  stylesheets="@../helpApp/HelpApp.css" BorderPane.alignment="CENTER">
                <Label fx:id="title" nodeOrientation="LEFT_TO_RIGHT" prefHeight="38.0" prefWidth="194.0"
                       text="进程管理器" HBox.hgrow="ALWAYS">
                    <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../img/process.png"/>
                        </ImageView>
                    </graphic>
                    <padding>
                        <Insets left="15.0"/>
                    </padding>
                </Label>
            </HBox>
        </left>
        <right>
            <HBox fx:id="titleBarR" alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308"
                  maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="55.0"
                  spacing="10.0" BorderPane.alignment="CENTER">
                <opaqueInsets>
                    <Insets/>
                </opaqueInsets>
                <cursor>
                    <Cursor fx:constant="DEFAULT"/>
                </cursor>
                <Button id="barIcon" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                        mnemonicParsing="false" onMouseClicked="#showDescription" prefHeight="35.0" prefWidth="35.0">
                    <graphic>
                        <ImageView id="barIcon" fitHeight="15.0" fitWidth="15.0" nodeOrientation="INHERIT"
                                   pickOnBounds="true" preserveRatio="true">

                            <cursor>
                                <Cursor fx:constant="DEFAULT"/>
                            </cursor>
                            <Image url="@../../img/help.png"/>
                        </ImageView>
                    </graphic>
                </Button>
                <Button id="barIcon" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                        mnemonicParsing="false" onMouseClicked="#minimizeStage" prefHeight="35.0" prefWidth="35.0">
                    <graphic>
                        <ImageView id="barIcon" fitHeight="15.0" fitWidth="15.0" nodeOrientation="INHERIT"
                                   pickOnBounds="true" preserveRatio="true">

                            <cursor>
                                <Cursor fx:constant="DEFAULT"/>
                            </cursor>
                            <Image url="@../../img/minimize0.png"/>
                        </ImageView>
                    </graphic>
                </Button>
                <Button id="barIcon" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                        mnemonicParsing="false" onMouseClicked="#resizeStage" prefHeight="0.0">
                    <graphic>
                        <ImageView disable="true" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <Image url="@../../img/change.png"/>
                        </ImageView>
                    </graphic>
                </Button>
                <Button id="barIcon" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" layoutX="774.0"
                        layoutY="15.0" mnemonicParsing="false" onMouseClicked="#closeStage" prefHeight="0.0">
                    <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">

                            <Image url="@../../img/close0.png"/>
                        </ImageView>
                    </graphic>
                </Button>
            </HBox>
        </right>
    </BorderPane>
    <AnchorPane fx:id="mainPane" layoutX="10.0" layoutY="47.0" stylesheets="@ProcessApp.css"
                minHeight="0.0" minWidth="0.0" prefHeight="437.0"
                prefWidth="1008.0" AnchorPane.bottomAnchor="5.0"
                AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="46.0">
        <TableView fx:id="processTable" layoutX="31.0" layoutY="83.0" nodeOrientation="LEFT_TO_RIGHT"
                   prefHeight="321.0" prefWidth="725.0" stylesheets="@ProcessApp.css" AnchorPane.bottomAnchor="30.0"
                   AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="252.0" AnchorPane.topAnchor="83.0">
            <columns>
                <TableColumn fx:id="processName" prefWidth="75.0" text="进程编号"/>
                <TableColumn fx:id="processState" prefWidth="75.0" text="进程状态"/>
                <TableColumn fx:id="whichFile" prefWidth="75.0" text="执行文件"/>
                <TableColumn fx:id="havedDevice" prefWidth="104.0" text="设备使用情况"/>
                <TableColumn fx:id="havedMemory" minWidth="0.0" prefWidth="124.0" text="内存占用情况"/>
                <TableColumn fx:id="havedPid" prefWidth="101.0" text="进程控制块"/>
                <TableColumn fx:id="result" prefWidth="109.0" text="当前执行结果"/>
                <TableColumn fx:id="progressBar" minWidth="0.0" prefWidth="124.0" text="进程完成进度"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <ToolBar layoutX="35.0" layoutY="21.0" prefHeight="41.0" prefWidth="725.0" stylesheets="@ProcessApp.css"
                 AnchorPane.leftAnchor="34.0" AnchorPane.rightAnchor="248.0" AnchorPane.topAnchor="21.0">
            <Button mnemonicParsing="false" prefHeight="23.0" prefWidth="149.0" text="正在运行进程的编号："/>
            <Button fx:id="nowProcessName" mnemonicParsing="false" prefHeight="23.0" prefWidth="60.0" text="1"/>
            <Button mnemonicParsing="false" prefHeight="23.0" prefWidth="123.0" text="当前执行结果："/>
            <Button fx:id="nowResult" mnemonicParsing="false" prefHeight="23.0" prefWidth="53.0" text="x=11"/>
            <Button mnemonicParsing="false" text="当前执行指令："/>
            <Button fx:id="nowInstruction" mnemonicParsing="false" text="x=11"/>
            <Button mnemonicParsing="false" text="剩余时间片："/>
            <Button fx:id="residueSlice" mnemonicParsing="false" text="x=11"/>
        </ToolBar>
        <TableView fx:id="nowProcessTable" layoutX="771.0" layoutY="21.0" nodeOrientation="LEFT_TO_RIGHT"
                   prefHeight="211.0" prefWidth="214.0" stylesheets="@ProcessApp.css" AnchorPane.rightAnchor="23.0"
                   AnchorPane.topAnchor="21.0">
            <columns>
                <TableColumn fx:id="instruction" prefWidth="186.0" text="当前进程执行进度"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <TabPane layoutX="772.0" layoutY="246.0" prefHeight="183.0" prefWidth="213.0" stylesheets="@ProcessApp.css"
                 tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="6.0" AnchorPane.rightAnchor="24.0"
                 AnchorPane.topAnchor="245.0">
            <Tab text="显示设置">
                <AnchorPane>
                    <CheckBox fx:id="showNow" layoutX="22.0" layoutY="24.0" mnemonicParsing="false"
                              onMouseClicked="#showSelectByMouse" text="当前进程"/>
                    <CheckBox fx:id="showCreating" layoutX="109.0" layoutY="24.0" mnemonicParsing="false"
                              onMouseClicked="#showSelectByMouse" text="新建进程"/>
                    <CheckBox fx:id="showWaiting" layoutX="22.0" layoutY="58.0" mnemonicParsing="false"
                              onMouseClicked="#showSelectByMouse" text="就绪进程"/>
                    <CheckBox fx:id="showBlocked" layoutX="109.0" layoutY="58.0" mnemonicParsing="false"
                              onMouseClicked="#showSelectByMouse" text="阻塞进程"/>
                    <CheckBox fx:id="showEnded" layoutX="22.0" layoutY="95.0" mnemonicParsing="false"
                              onMouseClicked="#showSelectByMouse" text="销毁进程"/>
                    <CheckBox fx:id="showAll" layoutX="109.0" layoutY="95.0" mnemonicParsing="false"
                              onMouseClicked="#showSelectByMouse" text="显示所有"/>
                </AnchorPane>
            </Tab>
            <Tab text="标记设置">
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <CheckBox fx:id="signEnded" layoutX="14.0" layoutY="90.0" mnemonicParsing="false"
                              onMouseClicked="#signSelectByMouse" text="销毁进程"/>
                    <CheckBox fx:id="signCreating" layoutX="14.0" layoutY="28.0" mnemonicParsing="false"
                              onMouseClicked="#signSelectByMouse" text="新建进程"/>
                    <CheckBox fx:id="signWaiting" layoutX="115.0" layoutY="28.0" mnemonicParsing="false"
                              onMouseClicked="#signSelectByMouse" text="就绪进程"/>
                    <CheckBox fx:id="signBlocked" layoutX="115.0" layoutY="59.0" mnemonicParsing="false"
                              onMouseClicked="#signSelectByMouse" text="阻塞进程"/>
                    <CheckBox fx:id="signRunning" layoutX="14.0" layoutY="59.0" mnemonicParsing="false"
                              onMouseClicked="#signSelectByMouse" text="运行进程"/>
                </AnchorPane>
            </Tab>
            <Tab text="进程控制">
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <CheckBox fx:id="continueButton" layoutX="67.0" layoutY="14.0" mnemonicParsing="false"
                              onMouseClicked="#createSelectByMouse" text="继续新建进程"/>
                    <CheckBox fx:id="suspendButton" layoutX="67.0" layoutY="47.0" mnemonicParsing="false"
                              onMouseClicked="#createSelectByMouse" text="暂停新建进程"/>
                    <RadioButton fx:id="speed1Button" layoutX="41.0" layoutY="82.0" mnemonicParsing="false"
                                 onMouseClicked="#speedSelectByMouse" text="1倍速"/>
                    <RadioButton fx:id="speed2Button" layoutX="134.0" layoutY="82.0" mnemonicParsing="false"
                                 onMouseClicked="#speedSelectByMouse" text="2倍速"/>
                    <RadioButton fx:id="speed4Button" layoutX="41.0" layoutY="112.0" mnemonicParsing="false"
                                 onMouseClicked="#speedSelectByMouse" text="4倍速"/>
                    <RadioButton fx:id="speed8Button" layoutX="134.0" layoutY="112.0" mnemonicParsing="false"
                                 onMouseClicked="#speedSelectByMouse" text="8倍速"/>
                </AnchorPane>
            </Tab>
        </TabPane>
    </AnchorPane>

</AnchorPane>
