<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<Pane fx:id="MainWindow" prefHeight="622.0" prefWidth="1035.0" stylesheets="@MainWindow.css"
      xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.os.main.MainController">

    <ImageView fx:id="background" fitHeight="599.0" fitWidth="1035.0" layoutY="14.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../img/background.png"/>
    </ImageView>
    <Pane id="buttonBarBackGround" fx:id="buttonBarBackGround" layoutX="-1.0" layoutY="464.0" prefHeight="105.0"
          prefWidth="1035.0"
          stylesheets="@MainWindow.css">
    </Pane>
    <!-- 任务栏 buttonBar -->
    <Pane id="buttonBar" fx:id="buttonBar" layoutX="-1.0" layoutY="464.0" prefHeight="105.0" prefWidth="1035.0"
          stylesheets="@MainWindow.css">
        <HBox id="appBox" fx:id="appBox" alignment="CENTER" layoutX="247.0" layoutY="13.0" prefHeight="84.0"
              prefWidth="392.0" stylesheets="@MainWindow.css">

            <!--         <Separator fx:id="separator22" opacity="0.0" orientation="VERTICAL" prefHeight="200.0"/>-->
            <!--      查看可执行文件 的按钮组件  -->
            <Button id="appButton" fx:id="executableFileButton" alignment="CENTER" contentDisplay="CENTER"
                    graphicTextGap="0.0" mnemonicParsing="false" prefHeight="62.0" prefWidth="78.0"
                    stylesheets="@MainWindow.css">
                <graphic>
                    <ImageView fitHeight="54.0" fitWidth="93.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/task.png"/>
                    </ImageView>
                </graphic>
            </Button>

            <!--         <Separator fx:id="separator23" opacity="0.0" orientation="VERTICAL" prefHeight="200.0"/>-->
            <!--        文件管理器 的按钮组件 -->
            <Button id="appButton" fx:id="fileManagerButton" alignment="CENTER" contentDisplay="CENTER"
                    graphicTextGap="0.0" mnemonicParsing="false" prefHeight="62.0" prefWidth="78.0"
                    stylesheets="@MainWindow.css">
                <graphic>
                    <ImageView fitHeight="54.0" fitWidth="93.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/fileManager.png"/>
                    </ImageView>
                </graphic>
            </Button>

            <!--         <Separator fx:id="separator24" opacity="0.0" orientation="VERTICAL" prefHeight="200.0"/>-->
            <Button id="appButton" fx:id="processButton" alignment="CENTER" contentDisplay="CENTER"
                    graphicTextGap="0.0" mnemonicParsing="false" textAlignment="CENTER">
                <graphic>
                    <ImageView fitHeight="64.0" fitWidth="54.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/process.png"/>
                    </ImageView>
                </graphic>
            </Button>

            <!--         <Separator fx:id="separator25" opacity="0.0" orientation="VERTICAL" prefHeight="200.0"/>-->
            <Button id="appButton" fx:id="occupancyButton" alignment="CENTER" contentDisplay="CENTER"
                    graphicTextGap="0.0" mnemonicParsing="false" prefHeight="68.0" prefWidth="55.0"
                    stylesheets="@MainWindow.css">
                <graphic>
                    <ImageView fitHeight="53.0" fitWidth="49.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/device.png"/>
                    </ImageView>
                </graphic>
            </Button>
        </HBox>

        <HBox id="appBox" fx:id="tipBox" alignment="CENTER" layoutX="634.0" layoutY="12.0" prefHeight="86.0"
              prefWidth="328.0" spacing="5.0" stylesheets="@MainWindow.css">
            <padding>
                <Insets right="16.0"/>
            </padding>

            <!--         <Separator fx:id="separator1" orientation="VERTICAL" prefHeight="86.0" prefWidth="66.0">-->
            <!--            <padding>-->
            <!--               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>-->
            <!--            </padding>-->
            <!--            <effect>-->
            <!--               <Blend/>-->
            <!--            </effect>-->
            <!--         </Separator>-->
            <Button id="controlButton" fx:id="helpButton" alignment="CENTER" contentDisplay="CENTER"
                    graphicTextGap="0.0" mnemonicParsing="false">
                <graphic>
                    <ImageView fitHeight="50.0" fitWidth="43.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/help.png"/>
                    </ImageView>
                </graphic>
            </Button>

            <Button id="controlButton" fx:id="minimizeButton" alignment="CENTER" contentDisplay="CENTER"
                    graphicTextGap="0.0" mnemonicParsing="false" onMouseClicked="#minimizeWindow" prefHeight="67.0"
                    prefWidth="54.0" stylesheets="@MainWindow.css">
                <graphic>
                    <ImageView fitHeight="39.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/minimize.png"/>
                    </ImageView>
                </graphic>
            </Button>

            <Button id="controlButton" fx:id="closeButton" alignment="CENTER" contentDisplay="CENTER"
                    graphicTextGap="0.0" mnemonicParsing="false" onMouseClicked="#closeWindow" prefHeight="50.0"
                    prefWidth="65.0" stylesheets="@MainWindow.css">
                <graphic>
                    <ImageView fitHeight="39.0" fitWidth="49.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/close.png"/>
                    </ImageView>
                </graphic>
            </Button>

            <VBox fx:id="timeBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                <Button id="timeButton1" fx:id="timeButton1" mnemonicParsing="false" stylesheets="@MainWindow.css"
                        text="Button"/>
                <Button id="timeButton2" fx:id="timeButton2" mnemonicParsing="false" stylesheets="@MainWindow.css"
                        text="Button"/>
            </VBox>

            <Button id="deskButton" fx:id="deskButton" alignment="CENTER" contentDisplay="CENTER"
                    graphicTextGap="0.0" mnemonicParsing="false" stylesheets="@MainWindow.css">
                <graphic>
                    <ImageView fitHeight="51.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/desktop.png"/>
                    </ImageView>
                </graphic>
            </Button>

        </HBox>
    </Pane>

</Pane>
