<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.BorderPane?>
<AnchorPane id="topMainPane" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" stylesheets="@../css/PropertyView.css"
            xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.os.apps.fileApp.Controller.PropertyCtl">
   <BorderPane fx:id="titleBar" layoutX="30.0" layoutY="30.0" stylesheets="@../css/PropertyView.css"
               onDragDone="#dragBarDone" onMouseDragged="#dragBar"
               onMousePressed="#pressBar" onMouseReleased="#releaseBar"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <left>
         <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0"
               prefWidth="134.0" stylesheets="@../css/PropertyView.css" BorderPane.alignment="CENTER">
            <Label fx:id="titleBarL" nodeOrientation="LEFT_TO_RIGHT" prefHeight="38.0" prefWidth="194.0" text="帮助"
                   HBox.hgrow="ALWAYS">
               <graphic>
                  <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                     <Image url="@../res/tip.png"/>
                  </ImageView>
               </graphic>
               <padding>
                  <Insets left="15.0"/>
               </padding>
            </Label>
         </HBox>
      </left>
      <right>
         <HBox fx:id="titleBarR" alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308"
               maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="55.0"
               spacing="10.0" BorderPane.alignment="CENTER">
            <opaqueInsets>
               <Insets/>
            </opaqueInsets>
            <cursor>
               <Cursor fx:constant="DEFAULT"/>
            </cursor>
            <Button id="barIcon" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                    mnemonicParsing="false" onMouseClicked="#minimizeStage" prefHeight="35.0" prefWidth="35.0">
               <graphic>
                  <ImageView id="barIcon" fitHeight="15.0" fitWidth="15.0" nodeOrientation="INHERIT"
                             pickOnBounds="true" preserveRatio="true">
                     <effect>
                        <DropShadow/>
                     </effect>
                     <cursor>
                        <Cursor fx:constant="DEFAULT"/>
                     </cursor>
                     <Image url="@../../../img/minimize0.png"/>
                  </ImageView>
               </graphic>
            </Button>
            <Button id="barIcon" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                    mnemonicParsing="false"  prefHeight="0.0">
               <graphic>
                  <ImageView disable="true" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true"
                             preserveRatio="true">
                     <effect>
                        <InnerShadow/>
                     </effect>
                     <Image url="@../../../img/squares.png"/>
                  </ImageView>
               </graphic>
            </Button>
            <Button id="barIcon" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" layoutX="774.0"
                    layoutY="15.0" mnemonicParsing="false" onMouseClicked="#closeStage" prefHeight="0.0">
               <graphic>
                  <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                     <effect>
                        <DropShadow/>
                     </effect>
                     <Image url="@../../../img/close.png"/>
                  </ImageView>
               </graphic>
            </Button>
         </HBox>
      </right>
   </BorderPane>
   <AnchorPane id="contentPane" prefHeight="300.0" prefWidth="300.0" stylesheets="@../css/PropertyView.css"
               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="604.0"
               AnchorPane.topAnchor="45.0">
      <FlowPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                prefHeight="300.0" prefWidth="290.0">
         <VBox prefHeight="300.0" prefWidth="300.0">
            <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                      prefHeight="250.0" prefWidth="290.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="152.199951171875" minWidth="10.0"
                                     prefWidth="84.60003662109375"/>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="196.00003662109373" minWidth="10.0"
                                     prefWidth="190.19996337890626"/>
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               </rowConstraints>
               <Label text="名称：">
                  <GridPane.margin>
                     <Insets left="8.0"/>
                  </GridPane.margin>
               </Label>
               <Label text="文件类型：" GridPane.rowIndex="1">
                  <GridPane.margin>
                     <Insets left="8.0"/>
                  </GridPane.margin>
               </Label>
               <Label text="位置：" GridPane.rowIndex="2">
                  <GridPane.margin>
                     <Insets left="8.0"/>
                  </GridPane.margin>
               </Label>
               <Label text="占用空间：" GridPane.rowIndex="3">
                  <GridPane.margin>
                     <Insets left="8.0"/>
                  </GridPane.margin>
               </Label>
               <Label text="建立时间：" GridPane.rowIndex="4">
                  <GridPane.margin>
                     <Insets left="8.0"/>
                  </GridPane.margin>
               </Label>
               <Label text="属性：" GridPane.rowIndex="5">
                  <GridPane.margin>
                     <Insets left="8.0"/>
                  </GridPane.margin>
               </Label>
               <Label fx:id="type" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
               <Label fx:id="loc" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
               <Label fx:id="space" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
               <Label fx:id="time" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
               <TextField fx:id="na" prefHeight="26.0" prefWidth="150.0"
                          style="-fx-border-color: #d3d3d3; -fx-border-radius: 5; -fx-background-color: #ffffff;"
                          GridPane.columnIndex="1"/>
               <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
                  <RadioButton fx:id="read" mnemonicParsing="false" prefHeight="5.0" text="只读">
                     <HBox.margin>
                        <Insets top="10.0"/>
                     </HBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="write" mnemonicParsing="false" prefHeight="5.0" text="读写">
                     <HBox.margin>
                        <Insets left="10.0" top="10.0"/>
                     </HBox.margin>
                  </RadioButton>
               </HBox>
            </GridPane>
            <HBox prefHeight="50.0" prefWidth="290.0">
               <Button fx:id="yes" mnemonicParsing="false" prefHeight="30.0" prefWidth="65.0" text="确定">
                  <HBox.margin>
                     <Insets left="33.0" top="10.0"/>
                  </HBox.margin>
               </Button>
               <Button fx:id="no" mnemonicParsing="false" prefHeight="30.0" prefWidth="65.0" text="取消">
                  <opaqueInsets>
                     <Insets/>
                  </opaqueInsets>
                  <HBox.margin>
                     <Insets left="20.0" top="10.0"/>
                  </HBox.margin>
               </Button>
               <Button fx:id="apply" mnemonicParsing="false" prefHeight="30.0" prefWidth="65.0" text="应用">
                  <HBox.margin>
                     <Insets left="20.0" top="10.0"/>
                  </HBox.margin>
               </Button>
            </HBox>
         </VBox>
      </FlowPane>
   </AnchorPane>
</AnchorPane>
